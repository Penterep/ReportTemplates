    <div class="page section1">
      <h1 style="padding-top:150px;">Report z bezpečnostního testu webové aplikace</h1>
      <div style="height: 150px;"></div>
      <hr />
      <h2>{$report->getProjectName()}</h2>
      <h2>{$report->getProjectUrl()}</h2>
      <hr />
      <div style="height: 150px;"></div>
      <h3>{$report->getReportCreateDate()}, verze {$report->getReportVersion()}</h3>
      <div style="height: 100px;"></div>
      <h3>{$report->getReportCreatorName()}</h3>
      <h3>{$report->getReportOwnerName()}</h3>
